{
  "ConnectionStrings": {
    "AnalysisDatabase": "Data Source=analysis.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AnalysisService": {
    "DefaultAlgorithmVersion": "1.0.0",
    "MaxConcurrentAnalysis": 20,
    "ResultCacheDurationMinutes": 60,
    "DetailedDiagnosticsEnabled": true
  }
}